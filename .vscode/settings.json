{
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.insertSpaces": false,
	"files.eol": "\n",
	"cSpell.words": [
		"builtins",
		"combinator",
		"comms",
		"cpuabuse",
		"errored",
		"jscc",
		"vuex",
		"pixi",
		"tock",
		"destructure",
		"destructured",
		"endregion",
		"trit",
		"vsocket"
	],
	"yaml.schemas": {
		"./schema/dt.yml.json": ["*.dt.yml"]
	},
	"files.associations": {
		"*.azure-pipelines.yml": "azure-pipelines"
	},
	"eslint.probe": ["typescript", "html", "vue"],
	"vetur.validation.templateProps": true,
	"editor.semanticTokenColorCustomizations": {
		"enabled": true,
		"rules": { "typeParameter": "#22AA22" }
	},
	"typescript.tsdk": "node_modules/typescript/lib"
}
