{
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.insertSpaces": false,
	"files.eol": "\n",
	"cSpell.words": [
		"builtins",
		"combinator",
		"comms",
		"cpuabuse",
		"jscc",
		"vuex",
		"pixi",
		"destructure",
		"destructured"
	],
	"yaml.schemas": {
		"./schema/dt.yml.json": ["*.dt.yml"]
	},
	"files.associations": {
		"*.azure-pipelines.yml": "azure-pipelines"
	},
	"eslint.probe": ["typescript", "html", "vue"],
	"vetur.validation.templateProps": true
}
